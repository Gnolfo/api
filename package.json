{
  "name": "api",
  "version": "1.0.0",
  "description": "API",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/jshint/bin/jshint --reporter=node_modules/jshint-stylish . && NODE_ENV=test mocha $(find ./test/ -name '*_test.js') --require test/bootstrap",
    "start": "npm run cleanup && node ./node_modules/forever/bin/forever start -w --minUptime 1000 --spinSleepTime 1000 -m 1 -l web-server.log -o ./web-server-stdout.log -e ./web-server-stderr.log index.js",
    "start:debug": "DEBUG=express:* ./node_modules/nodemon/bin/nodemon.js index.js",
    "stop": "node ./node_modules/forever/bin/forever stop -w --minUptime 1000 --spinSleepTime 1000 -m 1 -l web-server.log -o ./web-server-stdout.log -e ./web-server-stderr.log index.js",
    "migrate": "cd ./app && sequelize db:migrate && cd ../",
    "seed": "cd ./app && sequelize db:seed:all && cd ../",
    "cleanup": "rm ~/.forever/*.log && rm *.log",
    "elasticsearch:create": "node ./elasticsearch/create",
    "elasticsearch:update": "node ./elasticsearch/update",
    "elasticsearch:delete": "node ./elasticsearch/delete"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/api/api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/api/api/issues"
  },
  "homepage": "https://github.com/api/api#readme",
  "dependencies": {
    "async": "^1.4.2",
    "bcrypt": "^0.8.5",
    "bluebird": "^2.10.1",
    "body-parser": "^1.13.3",
    "bugsnag": "^1.6.6",
    "class-extend": "^0.1.2",
    "colors": "^1.1.2",
    "compression": "^1.5.2",
    "convict": "^1.0.1",
    "debug": "^2.2.0",
    "elasticsearch": "^8.2.0",
    "express": "^4.13.3",
    "express-jwt": "^3.3.0",
    "express-rate-limit": "git://github.com/manifestinteractive/express-rate-limit.git#d615bcf3cb50cb67e6a6c05d220d7b86d7f82255",
    "express-session": "^1.12.1",
    "forever": "^0.15.1",
    "hashids": "^1.0.2",
    "jsonwebtoken": "^5.5.0",
    "lodash": "^3.10.1",
    "mandrill-api": "^1.0.45",
    "md5": "^2.0.0",
    "moment": "^2.10.6",
    "mysql": "git://github.com/mysqljs/mysql.git#a0f2cec26ee86536dbc1c2837b92b191ca9618f1",
    "nodemon": "^1.4.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-localapikey": "0.0.3",
    "randomstring": "^1.1.3",
    "raven": "^0.8.1",
    "request": "^2.69.0",
    "sanitizer": "^0.1.2",
    "sequelize": "^3.19.0",
    "sequelize-cli": "2.3.1",
    "sequelize-fixtures": "^0.4.8",
    "sequelize-slugify": "^0.1.1",
    "slug": "^0.9.1",
    "uuid": "^2.0.1",
    "validate.js": "^0.9.0",
    "validator": "^4.0.6",
    "yargs": "^3.32.0"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "chai-passport-strategy": "^0.2.0",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "mocha": "^2.3.0",
    "rewire": "^2.5.1",
    "sinon": "^1.16.1"
  }
}
